    <comp lib="12" loc="(480,520)" name="RiscvMainDecoderExt">
      <a name="behavior">long Op = state.getPortValue(nameToId.get("Op")).toLongValue();
long Funct3 = state.getPortValue(nameToId.get("Funct3")).toLongValue();
long RegWrite = 0, ImmSrc = 0, ALUSrc = 0, MemWrite = 0, ResultSrc = 0, Branch = 0, ALUOp = 0, O1 = 0, RegLuiAuipc=0, Jump=0, PcOrReg=0, CsrWrite=0, Mret=0;

if(Op == 0x3) { // lw
    RegWrite = 1;
    ALUSrc = 1;
    ResultSrc = 1;
} else if(Op == 0x23) { // sw
    ImmSrc = 1;
    ALUSrc = 1;
    MemWrite = 1;
} else if(Op == 0x33) { // R-type
    RegWrite = 1;
    ALUOp = 2;
} else if(Op == 0x63) { // beq
    ImmSrc = 2;
    Branch = 1;
    ALUOp = 1;
} else if(Op == 0x13) { // addi
    RegWrite = 1;
    ALUSrc = 1;
    ResultSrc = 0;
    ALUOp = 2;
} else if(Op == 0x37) { //lui
     RegLuiAuipc=2;
     RegWrite = 1;
     ALUOp = 2;
     ALUSrc = 1;
     ImmSrc=3;
} else if(Op == 0x13) { //auipc
     RegLuiAuipc=1;
     RegWrite = 1;
     ALUOp = 2;
     ALUSrc = 1;
     ImmSrc=3;
} else if(Op == 0x6F) { //jal
     RegWrite=1;
     ImmSrc=4;
     ResultSrc=2;
     Jump=1;
} else if(Op == 0x67) { //jalr
     RegWrite=1;
     ResultSrc=2;
     Jump=1;
     PcOrReg=1;
}else if(Op == 0x73) { //csr
     if(Funct3==0){ //mret
         Mret=1;
     }
     if(Funct3==2){ //csrr
         RegWrite = 1;
         ResultSrc= 3;
     }
     else if(Funct3==1){ //csrw
         CsrWrite = 1;
     }
}
Value branchVal = Value.createKnown(BitWidth.create(1), Branch);
state.setPort(nameToId.get("Branch"), branchVal, 1);
Value resultSrcVal = Value.createKnown(BitWidth.create(2), ResultSrc);
state.setPort(nameToId.get("ResultSrc"), resultSrcVal, 2);
Value memWriteVal = Value.createKnown(BitWidth.create(1), MemWrite);
state.setPort(nameToId.get("MemWrite"), memWriteVal, 1);
Value aluSrcVal = Value.createKnown(BitWidth.create(1), ALUSrc);
state.setPort(nameToId.get("ALUSrc"), aluSrcVal, 1);
Value regWriteVal = Value.createKnown(BitWidth.create(1), RegWrite);
state.setPort(nameToId.get("RegWrite"), regWriteVal, 1);
Value immSrcVal = Value.createKnown(BitWidth.create(3), ImmSrc);
state.setPort(nameToId.get("ImmSrc"), immSrcVal, 3);
Value o1Val = Value.createKnown(BitWidth.create(1), O1);
state.setPort(nameToId.get("O1"), o1Val, 1);
Value regluiauipcVal = Value.createKnown(BitWidth.create(2), RegLuiAuipc);
state.setPort(nameToId.get("RegLuiAuipc"), regluiauipcVal, 1);
Value csrWriteVal = Value.createKnown(BitWidth.create(1), CsrWrite);
state.setPort(nameToId.get("CsrWrite"), csrWriteVal, 1);
Value jumpVal = Value.createKnown(BitWidth.create(1), Jump);
state.setPort(nameToId.get("Jump"), jumpVal, 1);
Value pcOrRegVal = Value.createKnown(BitWidth.create(1), PcOrReg);
state.setPort(nameToId.get("PcOrReg"), pcOrRegVal, 1);
Value aluOpVal = Value.createKnown(BitWidth.create(2), ALUOp);
state.setPort(nameToId.get("ALUOp"), aluOpVal, 2);
Value mretVal = Value.createKnown(BitWidth.create(1), Mret);
state.setPort(nameToId.get("Mret"), mretVal, 1);
</a>
    </comp>
    <comp lib="12" loc="(480,680)" name="RiscvALUDecoder">
      <a name="behavior">
 
long ALUOp = state.getPortValue(nameToId.get("ALUOp")).toLongValue();
long Op = state.getPortValue(nameToId.get("Op")).toLongValue();
long Funct3 = state.getPortValue(nameToId.get("Funct3")).toLongValue();
long Funct7 = state.getPortValue(nameToId.get("Funct7")).toLongValue();

long ans = 0;
 
if (ALUOp == 0x0) { // ALUOp = 00 for lw/sw
   ans = 0b000; // ALUControl = 000 (add) 
} else if (ALUOp == 0x1) { // ALUOp = 01 for beq
   ans = 0b001; // ALUControl = 001 (subtract) 
} else if (ALUOp == 0x2) { // ALUOp = 10 for R-type instructions
   if (Funct3 == 0x0) { 
      if(Op == 0x1 &amp;&amp; Funct7 == 0x20) { 
         ans = 0b001; // ALUControl = 001 (subtract)
      }else if(Funct7 == 0x1){
	 ans = 0b100; // ALUControl = 100 (multiply)
      } else {
         ans = 0b000; // ALUControl = 000 (add)
      }  
   } else if (Funct3 == 0x2) {   
      ans = 0b110; // ALUControl = 110 (set less than)
   } else if (Funct3 == 0x4 &amp;&amp; Funct7 == 0x1){
      ans = 0b101; // ALUControl = 101 (divide)
   } else if (Funct3 == 0x6) {   
      ans = 0b011; // ALUControl = 011 (or)   
   } else if (Funct3 == 0x7) {   
      ans = 0b010; // ALUControl = 010 (and)   
   }   
}   
 
Value ALUControl = Value.createKnown(BitWidth.create(3), ans);   
 
state.setPort(nameToId.get("ALUControl"), ALUControl, 3);  </a>
